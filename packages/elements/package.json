{
  "name": "@cleargdpr/elements",
  "description": "ClearGDPR Elements SDK",
  "version": "1.0.0",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "postinstall": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "node scripts/test.js --env=jsdom --watch"
  },
  "main": "dist/elements.js",
  "dependencies": {
    "@cleargdpr/js-sdk": "^1.0.0",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "cache-loader": "^1.2.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-to-clipboard": "^3.0.8",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "es6-promises": "^1.0.10",
    "eslint": "4.4.1",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "file-saver": "^1.3.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "informed": "^1.6.0",
    "jest": "20.0.4",
    "lodash": "^4.17.10",
    "moment": "^2.19.2",
    "node-sass": "^4.8.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.8.2",
    "promise": "8.0.1",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.4.1",
    "react-json-editor-ajrm": "^2.5.2",
    "react-responsive-modal": "^3.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "3.1.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "npm:@monastic.panic/enzyme-adapter-react-16",
    "enzyme-to-json": "^3.3.4",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-jest": "^21.17.0",
    "sinon": "^6.0.0"
  },
  "peerDependencies": {
    "react": "^16.4.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/requestAnimationFrame.js",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
